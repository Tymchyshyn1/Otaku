<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/ivImageStudioItem"
        android:layout_width="@dimen/item_details_studios_ivImageStudioItem_layout_width"
        android:layout_height="wrap_content"
        android:layout_marginVertical="@dimen/item_details_studios_ivImageStudioItem_layout_marginVertical"
        android:layout_marginEnd="@dimen/item_details_studios_ivImageStudioItem_layout_marginEnd"
        android:contentDescription="@string/item_details_studios_ivImageStudioItem_contentDescription"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>